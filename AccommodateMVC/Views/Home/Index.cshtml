@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>HOW MAY WE ACCOMMODATE YOU</p>
</div>

<div class="text-center">
    
    <ul style="list-style-type:none">
        
        @foreach (KeyValuePair<string, string> action in ViewBag.actions)
        {
            <li><a href="/@action.Key">@action.Value</a></li>
        }

    </ul>
</div>
